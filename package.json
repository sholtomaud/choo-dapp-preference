{
  "name": "choo-dapp-preference",
  "version": "1.0.0",
  "engines" : {
    "node" : "10.0"
  },
  "private": true,
  "scripts": {
    "build": "export NODE_ENV=production && bankai build index.js",
    "deploy": "npx now -e NODE_ENV=\"production\" --public",
    "dev": "export NODE_OPTIONS='--trace-warnings --pending-deprecation' && bankai start index.js",
    "create": "choo-scaffold",
    "inspect": "bankai inspect index.js",
    "start": "serve ./dist --single",
    "test": "standard && npm run test-deps",
    "test-deps": "dependency-check . && dependency-check . --extra --no-dev -i tachyons"
  },
  "dependencies": {
    "choo": "^6.8.0",
    "choo-devtools": "^2.3.4",
    "choo-service-worker": "^2.4.0",
    "cpjax": "^1.1.3",
    "crelns": "^1.0.1",
    "fastn":"shotlom/fastn",
    "localforage": "^1.7.1",
    "nanocomponent": "^6.5.1",
    "route-tree": "^3.0.0",
    "serve": "^14.2.1",
    "sheetify": "^7.3.0",
    "skylinkjs": "^0.6.30",
    "svg-icon-component": "^1.0.2",
    "tachyons": "^4.9.1",
    "upup": "^1.0.0"
  },
  "devDependencies": {
    "bankai": "^9.10.4",
    "choo-scaffold": "^1.1.2",
    "dependency-check": "^3.0.0",
    "standard": "^11.0.0"
  }
}
